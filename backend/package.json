{
  "name": "test-backend",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "node -r ./node_modules/typescript/bin/tsc",
    "start": "node -r ts-node/register ./src/server.ts",
    "start:watch": "nodemon --config nodemoon.json",
    "start_production": "node src/server.js",
    "migrate:create": "ts-node ./node_modules/typeorm/cli.js migration:create --config src/common/config/ormconfig.ts -n",
    "migrate:up": "ts-node ./node_modules/typeorm/cli.js migration:run --config src/common/config/ormconfig.ts",
    "migrate:down": "ts-node ./node_modules/typeorm/cli.js migration:revert --config src/common/config/ormconfig.ts"
  },
  "dependencies": {
    "@types/compression": "^1.7.0",
    "@types/dotenv": "^8.2.0",
    "@types/express-jwt": "^6.0.1",
    "@types/inversify": "^2.0.33",
    "@types/swagger-ui-express": "^4.1.2",
    "better-sqlite3": "^7.1.2",
    "class-validator": "^0.13.1",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-formidable": "^1.2.0",
    "express-jwt": "^6.0.0",
    "express-session": "^1.17.1",
    "helmet": "^4.4.1",
    "inversify": "^5.0.5",
    "inversify-binding-decorators": "^4.0.0",
    "inversify-express-utils": "6.1.0",
    "reflect-metadata": "^0.1.13",
    "swagger-express-ts": "^1.0.1",
    "swagger-ui-express": "^4.1.6",
    "twig": "1.15.4",
    "typeorm": "^0.2.31",
    "uuid": "^8.3.2",
    "ws": "^7.4.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/helmet": "^4.0.0",
    "@types/node": "^14.14.31",
    "eslint": "^7.21.0",
    "eslint-plugin-import": "^2.22.1",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  }
}
